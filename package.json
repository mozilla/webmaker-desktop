{
  "name": "webmaker-browser",
  "version": "1.6.2",
  "description": "Webmaker for Browsers",
  "repository": {
    "type": "git",
    "url": "https://github.com/mozilla/webmaker-browser"
  },
  "bugs": {
    "url": "https://github.com/mozilla/webmaker-browser/issues"
  },
  "keywords": [
    "mozilla",
    "webmaker"
  ],
  "author": "Mozilla",
  "license": "MPL-2.0",
  "engines": {
    "node": "4.4.x"
  },
  "dependencies": {
    "bunyan": "^1.8.0",
    "classnames": "^2.2.3",
    "color": "^0.11.1",
    "email-validator": "^1.0.4",
    "handlebars": "^4.0.5",
    "hapi": "^13.3.0",
    "hapi-bunyan": "^0.6.0",
    "nets": "^3.2.0",
    "react": "^15.0.1",
    "react-cookie": "^0.4.5",
    "react-dom": "^15.0.1",
    "react-intl": "^2.0.1",
    "react-router": "^2.3.0",
    "ua-parser-js": "^0.7.10",
    "webmaker-core": "https://github.com/mozilla/webmaker-core.git#v1.6.2",
    "object-assign": "^4.0.1"
  },
  "devDependencies": {
    "autoless": "^0.1.7",
    "babel-core": "^6.7.5",
    "babel-loader": "^6.2.4",
    "babel-preset-react": "^6.5.0",
    "babel-preset-es2015": "^6.6.0",
    "expect.js": "^0.3.1",
    "habitat": "^3.1.2",
    "json-loader": "^0.5.4",
    "jsx-loader": "^0.13.2",
    "eslint": "^2.8.0",
    "eslint-plugin-react": "^5.0.1",
    "mkdirp": "^0.5.1",
    "ncp": "^2.0.0",
    "npm-run-all": "^1.7.0",
    "rimraf": "^2.5.2",
    "webpack": "^1.13.0",
    "yaml-intl-xml-json-converter": "^0.0.7"
  },
  "scripts": {
    "start": "npm run build && npm run server",
    "postinstall": "npm run build",
    "dev": "npm-run-all --parallel server watch:**",
    "config": "node ./build-config.js > src/config.js",
    "build": "npm-run-all build:**",
    "build:clean": "rimraf ./build && mkdirp ./build",
    "build:static": "ncp ./static ./build",
    "build:l10njson": "yamlconvert intl-config.json",
    "build:css": "npm run watch:css -- --no-watch",
    "build:js": "npm run build:core && npm run config && webpack",
    "build:core": "node npm_tasks/copy-env.js && node npm_tasks/build-core.js",
    "watch:js": "npm run build:js -- --watch",
    "watch:css": "autoless --autoprefix \"> 5%, last 2 versions, android >= 4.0\" ./src ./build/css",
    "test": "npm run test:lint",
    "test:lint": "eslint --ext .js,.jsx src/",
    "server": "node server.js"
  }
}
